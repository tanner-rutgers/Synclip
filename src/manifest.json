{
  "manifest_version": 2,

  "name": "ClipSync",
  "description": "This extension allows sending text in your clipboard to other Chrome instances",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "19": "resources/icon19.png",
      "38": "resources/icon38.png"
    },
    "name": "Click to publish clipboard",
    "default_popup": "pages/popup.html"
  },

  "icons": {
    "16": "resources/icon16.png",
    "48": "resources/icon48.png",
    "128": "resources/icon128.png"
  },

  "background": {
    "page": "pages/background.html"
  },

  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "notifications",
    "gcm",
    "background"
  ],

  "key": ""
}